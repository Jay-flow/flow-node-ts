{
  "name": "flow-node-ts",
  "version": "1.0.0",
  "description": "Node project template.",
  "scripts": {
    "tsc": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "shx rm -rf dist/ && npm run tsc",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jay-flow/flow-node-ts.git"
  },
  "keywords": [
    "node"
  ],
  "author": "jay-flow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jay-flow/flow-node-ts/issues"
  },
  "homepage": "https://github.com/Jay-flow/flow-node-ts#readme",
  "devDependencies": {
    "@types/node": "^20.4.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.6.1"
}
